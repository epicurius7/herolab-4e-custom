    <tag group="ReqRace" tag="rSoEJotun"/>
    <tag group="Tier" tag="Heroic"/>
    </thing>
  <thing id="ftLurkLigh" name="Lurking Light" description="{b}Benefit:{/b} When one of your allies makes an attack granted by one of your powers, that attack has combat advantage against enemies from whom you are hidden." compset="Feat" summary="Allies get combat advantage on granted attacks while you are hidden." uniqueness="useronce">
    <fieldval field="reqText" value="Felbraug, Kobold or Goblin, Leader"/>
    <usesource source="SkEscarnum"/>
    <tag group="ReqRace" tag="rSoEFelbrg"/>
    <tag group="ReqRace" tag="rKobold"/>
    <tag group="ReqRole" tag="Leader"/>
