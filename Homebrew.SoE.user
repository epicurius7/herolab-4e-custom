  <thing id="ftVaeIngen" name="Vaess&apos; Ingenuity" description="{b}Benefit:{/b} Creatures you summon with arcane powers gain a +1 bonus per tier to attack." compset="Feat" summary="Arcane summons get +1 per tier to attack." uniqueness="useronce">
