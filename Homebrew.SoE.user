    <tag group="ReqRace" tag="rKobold"/>
    <tag group="Tier" tag="Heroic"/>
    <tag group="ReqRole" tag="Defender"/>
    </thing>
  <thing id="ftErthRenw" name="Earth&apos;s Renewal" description="{b}Benefit:{/b} Your total number of healing surges increases by two. You add your Constitution modifier to your healing surge value." compset="Feat" summary="+2 healing surges, surge value increased by Con mod." uniqueness="useronce">
    <fieldval field="reqText" value="Equitarn or Jotun, Paragon Tier"/>
    <usesource source="SkEscarnum"/>
    <tag group="Tier" tag="Paragon"/>
    <tag group="ReqRace" tag="rSoEEquitn"/>
    <tag group="ReqRace" tag="rSoEJotun"/>
    <eval phase="Traits" priority="10000"><![CDATA[      #traitmodify[trSurges,trtBonus,2,""]

      var bonus as number
      bonus = #attrbasebonus[attrCon]
      #traitmodify[trSurgeVal,trtBonus,bonus,""]]]>
      <before name="Derived trtFinal"/>
      <after name="Calc attrBonus"/>
      </eval>
    </thing>
  <thing id="ftEtrnCycl" name="Eternal Cycle" description="{b}Benefit:{/b} Your landfall power gains &quot;Sustain minor: the zone persists&quot;. The zone moves with you as you move." compset="Feat" summary="Landfall gains sustain minor, moves with you." uniqueness="useronce">
