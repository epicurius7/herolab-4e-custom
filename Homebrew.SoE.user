  <thing id="adjMultDai" name="Power Swap (Daily)" description="Swap one class daily power for a daily power from your multiclass. The substitute power&apos;s level cannot exceed one half of the original power&apos;s level." compset="Adjustment">
    <usesource source="SkEscarnum" parent="UserParent" name="Skies of Escarnum"/>
    <tag group="InPlay" tag="PermOK"/>
    <eval phase="Setup" priority="10000">~decrease our number of class daily powers by 1
#resmax[resDaily] -= 1

~increase our number of multiclass daily powers by 1
#resmax[resMCDai] += 1</eval>
    <exprreq message="Character must have at least one daily power."><![CDATA[#resmax[resDaily] > 0]]></exprreq>
