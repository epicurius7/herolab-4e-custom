  <thing id="pfArdOppSe" name="Opportunity Seized" description="{b}Trigger{/b}: You or one ally in the burst misses with an opportunity attack\n\n{b}Effect{/b}: The target makes another opportunity attack." compset="Power" replaces="pArdOppSei">
