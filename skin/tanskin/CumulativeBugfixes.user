  <thing id="ftGoliaGWP" name="Goliath Greatweapon Prowess" description="{b}Benefit{/b}: You gain proficiency with all simple and military two-handed melee weapons and a +2 feat bonus to damage rolls with such weapons. The bonus increases to +3 at 11th level and +4 at 21st level." compset="Feat" uniqueness="useronce">
    <fieldval field="reqText" value="Goliath"/>
    <usesource source="PHB2" parent="Supplement" name="Player&apos;s Handbook 2"/>
    <tag group="ReqRace" tag="rGoliath" name="Goliath" abbrev="Goliath"/>
    <tag group="Tier" tag="Heroic" name="Heroic Tier" abbrev="Heroic Tier"/>
    <eval phase="Traits" priority="1000"><![CDATA[
      var bonus as number
      if (#level[] <= 10) then
        bonus = 2
      elseif (#level[] <= 20) then
        bonus = 3
      else
        bonus = 4
      endif

      foreach pick in hero from WeaponBase where "(WepCat.wcMilitary | WepCat.wcSimple ) & Equipment.TwoHand"
        perform eachpick.field[wpDamage].modify[+,bonus,""]
        nexteach]]>
      <before name="Weapon proficiencies final"/>
      </eval>
    </thing>
