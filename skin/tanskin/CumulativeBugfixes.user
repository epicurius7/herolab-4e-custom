    <tag group="ArmorType" tag="arPlateArm" name="Plate Armor" abbrev="Plate Armor"/>
    <pickreq thing="apFullPlat"/>
    </thing>
  <thing id="arSpikPFix" name="Spiked Plate" description="A dwarven innovation, spiked plate is a suit of plate mail studded with sharp, metal spikes. The dwarves developed this armor to deal with subterranean beasts such as hook horrors and umber hulks that grasp and crush their prey." compset="Armor" replaces="arSpikePla">
